{
  "cleanUrls": true,
  "trailingSlash": false,
  "rewrites": [
    { "source": "/:tool(webp-to-jpg|webp-to-png|webp-to-ico|webp-to-bmp|webp-to-tiff|avif-to-jpg|avif-to-png|png-to-jpg|png-to-webp|jpg-to-png|pdf-to-word|pdf-to-excel)", "destination": "/tools/:tool.html" },
    { "source": "/:page(about|contact|privacy|terms|disclaimer)", "destination": "/pages/:page.html" },
    { "source": "/blog/:slug*", "destination": "/blog/:slug*.html" }
  ]
}
