{
  "cleanUrls": true,
  "trailingSlash": false,
  "rewrites": [
    { "source": "/(webp-to-jpg|webp-to-png|webp-to-ico|webp-to-bmp|webp-to-tiff|avif-to-jpg|avif-to-png|avif-to-webp|png-to-jpg|png-to-webp|png-to-png|jpg-to-png|jpg-to-webp|pdf-to-word|pdf-to-excel|pdf-to-ppt|heic-to-jpg|raw-to-jpg|tiff-to-jpg)", "destination": "/tools/$1.html" },
    { "source": "/(about|contact|privacy|terms|disclaimer|faq)", "destination": "/pages/$1.html" },
    { "source": "/blog", "destination": "/blog/index.html" },
    { "source": "/blog/([^/]+)", "destination": "/blog/$1.html" }
  ]
}
