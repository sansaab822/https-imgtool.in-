{
  "rewrites": [
    { "source": "/((?!api|tools|pages|blog|image|include|assets).*)", "destination": "/tools/$1.html" },
    { "source": "/about", "destination": "/pages/about.html" },
    { "source": "/contact", "destination": "/pages/contact.html" },
    { "source": "/blog", "destination": "/blog/index.html" }
  ],
  "cleanUrls": true,
  "trailingSlash": false
}
