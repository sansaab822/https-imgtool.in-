    { "source": "/pdf-crop", "destination": "/tools/pdf-crop.html" },
    { "source": "/bg-remover", "destination": "/tools/bg-remover.html" },
    { "source": "/image-compressor", "destination": "/tools/image-compressor.html" },
    { "source": "/image-resizer", "destination": "/tools/image-resizer.html" },
    { "source": "/crop-image", "destination": "/tools/crop-image.html" },
    { "source": "/passport-size-photo", "destination": "/tools/passport-size-photo.html" },
    { "source": "/image-enhancer", "destination": "/tools/image-enhancer.html" },
    { "source": "/image-to-art", "destination": "/tools/image-to-art.html" },
    { "source": "/3d-text-to-stl-generator", "destination": "/tools/3d-text-to-stl-generator.html" },
    { "source": "/all-image-converters", "destination": "/tools/all-image-converters.html" },
    { "source": "/about-us", "destination": "/about-us.html" },
    { "source": "/contact-us", "destination": "/contact-us.html" },
    { "source": "/privacy-policy", "destination": "/privacy-policy.html" },
    { "source": "/terms-of-service", "destination": "/terms-of-service.html" },
    { "source": "/blog", "destination": "/blog.html" }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        },
        {
          "key": "Referrer-Policy",
          "value": "strict-origin-when-cross-origin"
        },
        {
          "key": "Permissions-Policy",
          "value": "camera=(), microphone=(), geolocation=()"
        }
      ]
    },
    {
      "source": "/(.*\\.(js|css|png|jpg|jpeg|gif|ico|svg|webp|avif|woff|woff2|ttf|otf))",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ],
  "redirects": [
    {
      "source": "/index.html",
      "destination": "/",
      "permanent": true
    },
    {
      "source": "/tools/avif-to-jpg.html",
      "destination": "/avif-to-jpg",
      "permanent": true
    }
  ]
}
